{
  "name": "snowflake-ui",
  "version": "0.0.3",
  "description": "A UI library, built with Vue.js",
  "main": "dist/index.js",
  "scripts": {
    "start": "parcel index.html --no-cache",
    "dev-test": "parcel watch test/* --no-cache & karma start",
    "test": "parcel build test/* --no-cache --no-minify && karma start --single-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ReformedCola/snowflake-ui.git"
  },
  "keywords": [
    "Vue",
    "UI",
    "Components"
  ],
  "author": "Zhiyuan He",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ReformedCola/snowflake-ui/issues"
  },
  "homepage": "https://github.com/ReformedCola/snowflake-ui#readme",
  "dependencies": {
    "vue": "^2.6.12",
    "vue-hot-reload-api": "^2.3.4"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^3.2.0",
    "chai": "^4.2.0",
    "chai-spies": "^1.0.0",
    "karma": "^5.2.3",
    "karma-chai": "^0.1.0",
    "karma-chai-spies": "^0.1.4",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^2.0.1",
    "karma-sinon-chai": "^2.0.2",
    "mocha": "^8.1.3",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.26.11",
    "sinon": "^9.1.0",
    "sinon-chai": "^3.5.0",
    "typescript": "^4.0.3",
    "vue-template-compiler": "^2.6.12"
